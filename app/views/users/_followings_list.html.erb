<li id="user-
<%= followed.id %>
">
  
<%= link_to gravatar_for(followed.user, size: 50), followed.user %>

  <span class="user">
<%= link_to followed.user.name, followed.user %>
</span>
</li>


<% if false %>

<li id="user-
<%= following_user.id %>
">
  
<%= link_to gravatar_for(following_user.user, size: 50), following_user.user %>

  <span class="user">
<%= link_to following_user.user.name, following_user.user %>
</span>
</li>



<li id="micropost-
<%= micropost.id %>
">
  
<%= link_to gravatar_for(micropost.user, size: 50), micropost.user %>

  <span class="user">
<%= link_to micropost.user.name, micropost.user %>
</span>
  <span class="content">
<%= micropost.content %>
</span>
  <span class="timestamp">
    Posted 
<%= time_ago_in_words(micropost.created_at) %>
 ago.
  </span>
</li>
<% end %>